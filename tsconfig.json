{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2015",
        "outDir": "build",
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "experimentalDecorators": true
    },
    "exclude": ["node_modules", "buildout"]
}
